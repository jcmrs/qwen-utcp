{
  "extraction": {
    "default_depth": 2,
    "supported_file_types": [
      ".md",
      ".rst",
      ".txt",
      ".py",
      ".ts",
      ".js",
      ".go",
      ".rs",
      ".ex",
      ".json",
      ".yaml",
      ".yml"
    ],
    "content_filters": {
      "include_comments": true,
      "include_code_docs": true,
      "include_readmes": true,
      "include_specs": true
    },
    "knowledge_categories": [
      "technical",
      "conceptual",
      "evolutionary",
      "wisdom"
    ]
  },
  "processing": {
    "nlp_model": "en_core_web_sm",
    "concept_similarity_threshold": 0.7,
    "relationship_detection": true
  },
  "ai_optimization": {
    "generate_embeddings": true,
    "embedding_model": "all-MiniLM-L6-v2",
    "index_updates": "daily"
  },
  "repositories": [
    "utcp-specification",
    "python-utcp",
    "typescript-utcp",
    "go-utcp",
    "rs-utcp",
    "elixir-utcp",
    "utcp-mcp",
    "go-utcp-mcp-bridge",
    "code-mode",
    "agent-implementation-example",
    "utcp-examples",
    "langchain-utcp-adapters",
    "pydantic-ai-utcp",
    "strands-utcp",
    "chat-utcp",
    "benchmarks",
    "utcp-agent"
  ],
  "output": {
    "raw_extractions_dir": "raw-extractions",
    "processed_knowledge_dir": "processed-knowledge",
    "ai_optimized_dir": "ai-optimized",
    "metadata_dir": "metadata",
    "wisdom_dir": "wisdom"
  },
  "iterative_processing": {
    "enabled": true,
    "max_iterations": 5,
    "expansion_factor": 1.5,
    "progressive_depth": true
  },
  "selective_processing": {
    "enabled": true,
    "default_selection": "all",
    "topic_based_extraction": true,
    "custom_filters": []
  }
}